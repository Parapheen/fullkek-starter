package user

import "time"

// User represents an authenticated person in the system.
type User struct {
    ID        string
    Email     string
    Name      string
    AvatarURL string
    CreatedAt time.Time
}

// Identity represents an external OAuth identity linked to a user.
type Identity struct {
    ID        string
    UserID    string
    Provider  string
    Subject   string
    CreatedAt time.Time
}

